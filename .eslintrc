{
  "env": {
    "browser": true,
    "node": true,
    "es6": true
  },
  "extends": "airbnb-base",
  "parser": "babel-eslint",
  "rules": {
    "import/no-extraneous-dependencies": [
      "error", 
      {
        "devDependencies": true
      }
    ],
    "import/no-webpack-loader-syntax": 0,
    "import/no-unresolved": 0,
    "max-len": 0
  }
}